<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - VotingWebApp</title>

    <link rel="stylesheet" href="~/css/material-dashboard.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link href="https://fonts.googleapis.com/css?family=Quicksand&display=swap" rel="stylesheet">
</head>
<body style="background-blend-mode:multiply;background:url('images/Voting-Front.jpg'),rgba(0,0,0,0.2);margin-bottom:0px">
    <ul class="nav" style="background-color:lightgrey;padding:0.5em 20em 0.5em 20em;border-bottom:2px solid white">
        <h2 style="font-family:'Quicksand',sans-serif;margin-bottom:0px">Online Voting</h2>
    </ul>
    
    <div class="container">
        @RenderBody()
    </div>

    <script src="~/js/jquery.min.js" type="text/javascript"></script>
    <script src="~/js/popper.min.js" type="text/javascript"></script>
    <script src="~/js/bootstrap-material-design.min.js" type="text/javascript"></script>
    <script src="~/js/perfect-scrollbar.jquery.min.js"></script>
    <script src="~/js/material-dashboard.min.js" type="text/javascript"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
